{
  "kind": "collectionType",
  "collectionName": "order_details",
  "info": {
    "singularName": "order-detail",
    "pluralName": "order-details",
    "displayName": "OrderDetail"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "order_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "order_details"
    },
    "product_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "order_details"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "quantity": {
      "type": "decimal",
      "required": true
    }
  }
}
